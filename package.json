{
  "name": "testing-engineer-project",
  "version": "1.0.0",
  "description": "A comprehensive testing project for learning software testing",
  "main": "index.js",
  "scripts": {
    "start": "python app/app.py",
    "test": "pytest tests/",
    "test:ui": "pytest tests/ui/",
    "test:api": "pytest tests/automation/api/",
    "test:performance": "locust -f tests/performance/locustfile.py",
    "test:report": "pytest --html=reports/report.html --self-contained-html",
    "test:allure": "pytest --alluredir=reports/allure-results",
    "allure:serve": "allure serve reports/allure-results",
    "lint": "flake8 tests/ app/",
    "format": "black tests/ app/",
    "install:python": "pip install -r requirements.txt",
    "setup": "npm run install:python && npm install"
  },
  "keywords": [
    "testing",
    "automation",
    "selenium",
    "pytest",
    "flask",
    "api-testing",
    "ui-testing",
    "performance-testing",
    "software-testing",
    "test-automation",
    "quality-assurance",
    "continuous-integration",
    "test-driven-development"
  ],
  "author": "Testing Engineer",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0",
    "python": ">=3.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AutumnStart/Ces.git"
  },
  "bugs": {
    "url": "https://github.com/AutumnStart/Ces/issues"
  },
  "homepage": "https://github.com/AutumnStart/Ces#readme"
}